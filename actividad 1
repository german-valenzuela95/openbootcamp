public class Main {
    public static void main(String[] args) {
        funcionsuma(8,4,5);
        coche miCoche= new coche();
        miCoche.incremeto();
        System.out.println("mi puerta es: " + miCoche.puerta);
    }
    public static void funcionsuma(int a,int b, int c){
      int resultado= a+b+c;
      System.out.println("resultado: " +resultado);
    }
}
     class coche{
       int puerta=0;

    public void incremeto() {
           this.puerta++;
    }
     }
